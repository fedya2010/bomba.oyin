import random
m=random.randint(0,10)
imkoniyat=5
a =[1,2,3,4,5,6,7,8,9]
b =0
c = 'x'
while True:
    for i in range(3):
        print(a[i*3],a[1+i*3], a[2+i*3]) 
    n= input(f'qayerga yurmoqchisiz\n{c}')
    n= int(n)
    n-=1
    a[n] = c
    if c==a[m]:
        print(f'oyin tugadi\n bomba{m}da edi')
        break
    if c!=m:
        imkoniyat-=1
        print ('davom eting')

    if imkoniyat==1:
        print('sen yutding')
        break
